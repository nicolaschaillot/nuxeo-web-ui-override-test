<?xml version="1.0" encoding="UTF-8"?>

<component name="org.keendoo.web.ui.resources">

  <require>org.nuxeo.web.ui.resources</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">

    <resource name="keendoo-elements.html" type="import">
      <uri>ui/keendoo-elements.html</uri>
    </resource>

  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">

    <!-- main bundle with all import resources -->
    <bundle name="web-ui">
      <resources append="true">
        <resource>keendoo-elements.html</resource>
      </resources>
    </bundle>

  </extension>

</component>
